@model DSRHWebTeste.Models.UserToken

@{
    ViewData["Title"] = "GerarToken";
}

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="GerarToken">
            <div class="form-group">
                <dl class="row">
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Token)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Token)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Expiration)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Expiration)
                    </dd>
                </dl>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
